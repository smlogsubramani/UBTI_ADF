{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-TRAINING-LS"
		},
		"ADFSERVER_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ADFSERVER'"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"TASK3BLOBSTORAGE_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'TASK3BLOBSTORAGE'"
		},
		"logasqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'logasqldb'"
		},
		"restservicetest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://reqres.in/api/users"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-TRAINING-LS_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"logasqldb_connectionString": {
						"value": "[parameters('logasqldb_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADF-TRAINING-LS_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADFSERVER_connectionString": {
						"value": "[parameters('ADFSERVER_connectionString')]"
					},
					"TASK3BLOBSTORAGE_connectionString": {
						"value": "[parameters('TASK3BLOBSTORAGE_connectionString')]"
					},
					"restservicetest_properties_typeProperties_url": {
						"value": "[parameters('restservicetest_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-TRAINING-LS_ArmTemplate_0')]"
			]
		},
		{
			"name": "ADF-TRAINING-LS_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-TRAINING-LS_ArmTemplate_1')]"
			]
		}
	]
}